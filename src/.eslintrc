{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "semi": 0,
        "quotes": [
            "error",
            "double"
        ],
        "eqeqeq": 2,
        "no-trailing-spaces": 2
    }
}